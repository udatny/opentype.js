/**
 * Check if a char is Arabic
 * @param {string} c a single char
 */
export function isArabicChar(c: string): boolean;
/**
 * Check if a char is an isolated arabic char
 * @param {string} c a single char
 */
export function isIsolatedArabicChar(char: any): boolean;
/**
 * Check if a char is an Arabic Tashkeel char
 * @param {string} c a single char
 */
export function isTashkeelArabicChar(char: any): boolean;
/**
 * Check if a char is Thai
 * @param {string} c a single char
 */
export function isThaiChar(c: string): boolean;
/**
 * Check if a char is Latin
 * @param {string} c a single char
 */
export function isLatinChar(c: string): boolean;
/**
 * Check if a char is whitespace char
 * @param {string} c a single char
 */
export function isWhiteSpace(c: string): boolean;
